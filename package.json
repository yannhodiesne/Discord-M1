{
  "name": "Discord",
  "version": "1.4.1",
  "main": "index.js",
  "description": "Discord Native for M1 Macs",
  "author": {
    "name": "17hoehbr",
    "email": "17hoehbr@gmail.com",
    "url": "https://github.com/17hoehbr/Discord-m1"
  },
  "contributors": [
    {
      "name": "Yann Hodiesne",
      "email": "yann.hodiesne@viacesi.fr",
      "url": "https://github.com/yannhodiesne/Discord-m1"
    }
  ],
  "homepage": "https://github.com/yannhodiesne/Discord-m1",
  "keywords": [
    "Discord"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com:yannhodiesne/Discord-m1"
  },
  "license": "MIT",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron --disable-gpu .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "dependencies": {
    "electron-prompt": "1.6.2",
    "electron-updater": "4.3.9",
    "electron-window-state": "5.0.3",
    "express": "4.17.1",
    "express-asset-file-cache-middleware": "1.3.0",
    "mac-screen-capture-permissions": "^2.0.0",
    "v8-compile-cache": "2.3.0"
  },
  "devDependencies": {
    "electron": "13.3.0",
    "electron-builder": "22.11.5",
    "electron-webpack": "2.8.2",
    "webpack": "4.42.1"
  },
  "build": {
    "appId": "Discord-m1",
    "extends": null,
    "extraResources": [
      "discord-native-features.js",
      "discord-platform-osx.js"
    ],
    "mac": {
      "category": "public.app-category.social-networking",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "arm64"
          ]
        }
      ],
      "icon": "icon.icns",
      "darkModeSupport": "true",
      "publish": [
        {
          "provider": "github",
          "owner": "yannhodiesne",
          "repo": "Discord-m1"
        }
      ]
    }
  }
}
